#define MAX 10  // constant /* with ignored comment */

/* the main /*
 * function
 * and this is // not comment
*/
int main() {
  char buf [MAX /* size of buffer */];
  int a = 10; /\
/ this is comment
  int b = 11; /\
* this is another valid comment *\
/
  const char* str1 = "This is // not comment";
  const char* str2 = "This is /* not comment too */";
};
